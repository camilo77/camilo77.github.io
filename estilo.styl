rojo = #37B821
colorTitulo = #FFFFFF
colorFondo = #FFFFFF
colorNavegador = #484A40

h1.titulo
    top 100px
    color colorTitulo

body
    background colorFondo
    padding 0px
    margin 0px

.navegador
    background colorNavegador
    height 80px
    width 100%
    opacity 0.8
    position fixed
    top 0px

.fondo-principal
    position fixed
    top -10px
    z-index -1
    width 100%


navegadores(propiedad, argumentos)
    -webkit-{propiedad} argumentos
    -moz-{propiedad} argumentos
    {propiedad} argumentos

.contenedor
    border 	2px
    .contenedor1
    	background rojo 

border-radiu()
	-webkit-border-radius arguments
	-moz-border-radius arguments
	border-radius arguments

a.button
	border-radiu 5px

